{
    "description": "right_command + Appname to open it",
    "manipulators": [
        {
            "from": {
                "key_code": "v",
                "modifiers": {
                    "mandatory": [
                        "left_command",
                        "left_control",
                        "left_option",
                        "left_shift"
                    ]
                }
            },
            "to": [
                {
                    "shell_command": "open -a 'Visual Studio Code.app'"
                }
            ],
            "type": "basic"
        }
    ]
}